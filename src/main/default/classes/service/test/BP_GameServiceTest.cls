@IsTest
public with sharing class BP_GameServiceTest {

    @IsTest
    private static void prepareRequestToBggSingleRecordTest() {
        final HttpRequest request = BP_GameService.newInstance().prepareRequestToBgg(false, null, 1000);
        Assert.areEqual('GET', request.getMethod());
        Assert.areEqual(BP_Constants.BGG_ENDPOINT + BP_Constants.GAME_HTTP_REQUEST_TYPE + '1000', request.getEndpoint());
    }

    @IsTest
    private static void prepareRequestToBggSearchTest() {
        final HttpRequest request = BP_GameService.newInstance().prepareRequestToBgg(true, 'Ark Nova', null);
        Assert.areEqual('GET', request.getMethod());
        Assert.areEqual(BP_Constants.BGG_ENDPOINT + BP_Constants.SEARCH_HTTP_REQUEST_TYPE + 'Ark Nova', request.getEndpoint());
    }

